<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.selfsoft.secrity.model.TmSysRegedit" table="TM_SYS_REGEDIT"
    dynamic-insert="true"
    dynamic-update="true"
    >
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="sysSn" type="java.lang.String">
            <column name="SYS_SN"/>
        </property>
    </class>
</hibernate-mapping>
